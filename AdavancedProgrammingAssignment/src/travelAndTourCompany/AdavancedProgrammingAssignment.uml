			case 1:&#xD;
				AdminState as = new AdminState();&#xD;
				as.setContext(c);&#xD;
				c.getState().login();&#xD;
				break;&#xD;
			case 2:&#xD;
				CustomerState cs = new CustomerState();&#xD;
				cs.setContext(c);&#xD;
