        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_O1L2cmEeEe6yhvWkSm6utg" name="login" specification="_OxhecGEeEe6yhvWkSm6utg">
          <language>java</language>
          <body>System.out.println(&quot;Welcome to Customer Login&quot;);&#xD;
		System.out.print(&quot;Enter username: &quot;);&#xD;
		String usn = sc.nextLine();&#xD;
		System.out.print(&quot;Enter password: &quot;);&#xD;
